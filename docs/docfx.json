{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": ["**/**.csproj"],
          "exclude": [
            "**/**.UnitTests.csproj",
            "**/**.Benchmarks.csproj",
            "**/bin/**",
            "**/obj/**",
            "_site/**"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["articles/**.md", "articles/**/toc.yml", "toc.yml", "*.md"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "output": "_site",
    "globalMetadata": {
      "_appTitle": "NetFabric.Numerics",
      "_appLogoPath": "images/Logo.png",
      "_appFaviconPath": "images/Logo.png",
      "_appFooter": "Copyright (c) 2023-2024 Ant√£o Almada",
      "_googleAnalyticsTagId": "G-E372L91FWS"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "modern", "template"],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false,
    "sitemap": {
      "baseUrl": "https://netfabric.github.io/NetFabric.Numerics.Tensors"
    }
  }
}
